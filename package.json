{
  "private": true,
  "name": "@knowler/www",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:sass build:elements build:remix",
    "build:remix": "remix build",
    "build:sass": "sass --load-path=styles app --style=compressed",
    "build:elements": "NODE_ENV=production node ./scripts/build-elements.mjs",
    "deploy": "fly deploy --remote-only",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:sass": "sass --load-path=styles --watch app",
    "dev:elements": "NODE_ENV=development nodemon -w ./scripts/build-elements.mjs ./scripts/build-elements.mjs",
    "dev:quirrel": "DISABLE_TELEMETRY=true quirrel",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@lexical/code": "0.5.0",
    "@lexical/history": "0.5.0",
    "@lexical/link": "0.5.0",
